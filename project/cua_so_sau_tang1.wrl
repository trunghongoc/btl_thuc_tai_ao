    translation 0.3 0 0
    children [
        Shape {
            geometry Box { size 0.1 2.1 0.1}
            appearance Appearance {
                material Material{
                    diffuseColor 0.9 0.9 0.9
                }   
            }
        }
    ]
}

# thanh dọc trai
Transform {
